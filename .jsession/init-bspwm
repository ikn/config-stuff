xrdb .Xresources
xset r rate 250 60
xsetroot -cursor_name left_ptr
setxkbmap gb dvorakukp -option -option caps:escape -option terminate:ctrl_alt_bksp
xset -dpms
xset s off
redshift -l 52.2:0.1
xrandr --output HDMI2 --right-of HDMI1
killall pulseaudio; sleep 2 && pulseaudio

sxhkd -c .config/sxhkd/sxhkdrc-bspwm -t 0.2
bspwm

urxvtd
bin/specific/set-wallpaper
kupfer --no-splash
sleep 2 && runpanel
maintain-fullscreen
