own_window yes
own_window_type normal
own_window_transparent yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
double_buffer yes
draw_shades no
draw_borders no

alignment bottom_right
gap_x 20
gap_y 35
use_spacer none

use_xft yes
xftfont Sans:pixelsize=11
default_color 141
no_buffers yes

TEXT
${font :pixelsize=11:Bold}HDD$font ${hr 2}
/ $alignc ${fs_used /} / ${fs_size /} $alignr ${fs_free_perc /}%
${fs_bar /}
/home $alignc ${fs_used /home} / ${fs_size /home} $alignr ${fs_free_perc /home}%
${fs_bar /home}
Data $alignc ${fs_used /media/Data} / ${fs_size /media/Data} $alignr ${fs_free_perc /media/Data}%
${fs_bar /media/Data}

${font :pixelsize=11:Bold}MEMORY$font ${hr 2}
$mem / $memmax $alignr $memperc%
$membar

${font :pixelsize=11:Bold}CPU$font ${hr 2}
${loadavg}
${execi 5 /home/j/bin/specific/temp 2}째C ${execi 5 /home/j/bin/specific/temp 3}째C ${execi 5 /home/j/bin/specific/temp 4}째C ${execi 5 /home/j/bin/specific/temp 5}째C
${cpugraph cpu0}
${cpu cpu1}% ${cpubar cpu1 6,50} $alignr ${cpu cpu2}% ${cpubar cpu2 6,50}
${cpu cpu3}% ${cpubar cpu3 6,50} $alignr ${cpu cpu4}% ${cpubar cpu4 6,50}
${cpu cpu5}% ${cpubar cpu5 6,50} $alignr ${cpu cpu6}% ${cpubar cpu6 6,50}
${cpu cpu7}% ${cpubar cpu7 6,50} $alignr ${cpu cpu8}% ${cpubar cpu8 6,50}

${font :pixelsize=11:Bold}NETWORK$font ${hr 2}
down: ${downspeedf enp3s0} KiB/s (${totaldown enp3s0})
up: ${upspeedf enp3s0} KiB/s (${totalup enp3s0})

${font :pixelsize=11:Bold}RUNTIME$font ${hr 2}
up ${uptime_short}

${font :pixelsize=22}${time %a %d %b}
